{
	"extends": "@tsconfig/node16/tsconfig.json",
	"ts-node": {
		"moduleTypes": {
			"jest.config.ts": "cjs"
		}
	},
	"compilerOptions": {
		"strict": true,
		"removeComments": false,
		"noImplicitAny": false,
		"declaration": true,
		"sourceMap": true,
		"outDir": "./dist",
		"paths": {
			"@jest/": ["./node_modules/@jest"]
		}
	}
}
